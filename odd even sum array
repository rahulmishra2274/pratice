#include<iostream>
using namespace std;
int main()
{
	int i,odd= 0, even = 0,size;
    cout<<"enter the size";
    cin>>size;
	int arr[size];
	for(int i =0;i<size;i++)
	{
	    cin>>arr[i];
	}
		for(int i =0;i<size;i++)
	{
	    if(arr[i]%2==0){
	        even = even + arr[i];
	    }
	    else {
	        odd = odd+ arr[i];
	    }
	}
	cout<<"odd element of sum "<<odd;
	cout<<endl<<"even element sum "<<even;
	return 0;
}
