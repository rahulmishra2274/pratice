#include<iostream>
using namespace std;
int main()
{
	int i,large,size;
    cout<<"enter the size";
    cin>>size;
	int arr[size];
	for(int i =0;i<size;i++){
		cin>>arr[i];
	}
	large=arr[0];
	for( i=0;i<size;i++)
	{
		if(arr[i]>large)
		{
			large = arr[i];
		}
	}
	cout<<"largest element of array"<<large;
	return 0;
}
